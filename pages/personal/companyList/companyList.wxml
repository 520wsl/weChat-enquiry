<!--pages/personal/companyList/companyList.wxml-->
<view class='company'>
  <view class='list-box'>
    <view class='list' wx:for="{{lists}}" wx:for-index="idx" wx:for-item="itemName" wx:key="idx">
      <text class='list_text'>{{idx}}{{itemName.name}}</text>
      <image src='http://1.img.dianjiangla.com/enquiryAssets/icon_right.png' class='icon_right'/>
    </view>
  </view>
</view>
