/* pages/enquiry/enquiry.wxss */
/* 时间 */
.time-range {
  display: flex;
  font-size: 28rpx;
  color: #999;
  text-align: center;
  padding: 20rpx 30rpx 15rpx;
  background: #fff;
  border-top: 1rpx solid #B2B2B2;
  border-bottom: 1rpx solid #E0E0E0;
}

.time-range .start-time, .time-range .end-time {
  flex: 1;
  border-bottom: 1rpx solid #e0e0e0;
}

.time-range .start-time.ac, .time-range .end-time.ac {
  color: #fc7c1f;
  border-color: #fc7c1f;
}

.time-range .txt {
  font-size: 28rpx;
  color: #333;
  padding: 0 45rpx;
}

